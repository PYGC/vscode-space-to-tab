{
	"name": "spacetab",
	"publisher": "PYGC",
	"displayName": "Space To Tab",
	"description": "Convert space to tab.",
	"version": "0.0.2",
	"repository": {
		"type": "git",
		"url": "https://github.com/PYGC/vscode-space-to-tab"
	},
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:spacetab.convert"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "spacetab.convert",
				"title": "Space Tab: Convert spaces to indentation using tabs."
			}
		],
		"keybindings": [
			{
				"command": "spacetab.convert",
				"key": "ctrl+t ctrl+t",
				"mac": "cmd+t cmd+t",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.13.29",
		"@types/vscode": "^1.50.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.9.7",
		"vscode-test": "^1.3.0"
	}
}
